{
	"name": "@nhealth/fhir",
	"version": "0.0.9",
	"description": "Create FHIR applications with Nuxt based on Medplum Fhir server",
	"repository": {
		"type": "git",
		"url": "git+https://github.com/DevJoghurt/fhir.git",
		"directory": "packages/ui"
	},
    "license": "MIT",
    "type": "module",
	"exports": {
		".": {
		  "types": "./dist/types.d.ts",
		  "import": "./dist/module.mjs",
		  "require": "./dist/module.cjs"
		}
	},
	"publishConfig": {
		"access": "public"
	},
	"main": "./dist/module.cjs",
	"types": "./dist/types.d.ts",
	"files": [
		"dist"
	],
	"scripts": {
		"prepack": "nuxt-module-build build",
		"prepack:stub": "nuxt-module-build build --stub"
	},
	"dependencies": {
		"@nuxt/kit": "3.13.2",
		"@nuxt/ui": "next",
		"@medplum/core": "3.2.16",
		"@medplum/fhirtypes": "3.2.16",
		"tailwindcss": "4.0.0-alpha.29",
		"hookable": "^5.5.3",
		"defu": "^6.1.4"
	},
	"devDependencies": {
		"@nuxt/module-builder": "0.8.4"
	}
}