{
	"name": "@nhealth/fhir-profiling",
	"version": "0.0.28",
	"description": "FHIR profiling module for Nuxt based on sushi and fsh",
	"repository": {
		"type": "git",
		"url": "git+https://github.com/DevJoghurt/fhir.git",
		"directory": "packages/profiling"
	},
    "license": "MIT",
    "type": "module",
	"exports": {
		".": {
			"types": "./dist/profiling.d.ts",
			"import": "./dist/profiling.mjs"
		},
		"./module": {
		  "types": "./dist/types.d.ts",
		  "import": "./dist/module.mjs",
		  "require": "./dist/module.cjs"
		},
		"./types": {
			"types": "./dist/profiling.d.ts"
		}
	},
	"publishConfig": {
		"access": "public"
	},
	"main": "./dist/module.cjs",
	"types": "./dist/types.d.ts",
	"files": [
		"dist"
	],
	"scripts": {
		"prepack": "nuxt-module-build build",
		"prepack:stub": "nuxt-module-build build --stub"
	},
	"dependencies": {
		"@nuxt/kit": "*",
		"@nuxt/schema": "*",
		"@nuxt/ui": "3.0.0-alpha.12",
		"@nuxt/content": "2.13.2",
		"@medplum/fhirtypes": "^3.2.30",
		"@nuxt/icon": "^1.10.3",
		"@vueuse/core": "^12.5.0",
		"radix-vue": "^1.9.13",
		"fsh-sushi": "3.11.1",
		"globby": "^14.0.2",
		"mdbox": "^0.1.1",
		"defu": "^6.1.4",
		"@nuxt/image": "^1.9.0",
		"@iconify/utils": "^2.2.1",
		"chokidar": "4.0.3",
		"fs-extra": "^11.3.0",
		"c12": "^2.0.1",
		"shiki": "^2.3.0"
	},
	"devDependencies": {
		"typescript": "^5.7.3",
		"@nuxt/module-builder": "0.8.4",
		"@types/fs-extra": "11.0.4",
		"unbuild": "^3.3.1"
	}
}