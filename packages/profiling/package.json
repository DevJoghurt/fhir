{
	"name": "@nhealth/fhir-profiling",
	"version": "0.0.9",
	"description": "FHIR profiling module for Nuxt based on sushi and fsh",
	"repository": {
		"type": "git",
		"url": "git+https://github.com/DevJoghurt/fhir.git",
		"directory": "packages/fhir-profiling"
	},
    "license": "MIT",
    "type": "module",
	"exports": {
		".": {
			"types": "./dist/profiling.d.ts",
			"import": "./dist/profiling.mjs"
		},
		"./module": {
		  "types": "./dist/types.d.ts",
		  "import": "./dist/module.mjs",
		  "require": "./dist/module.cjs"
		},
		"./types": {
			"types": "./dist/profiling.d.ts"
		}
	},
	"publishConfig": {
		"access": "public"
	},
	"main": "./dist/module.cjs",
	"types": "./dist/types.d.ts",
	"files": [
		"dist"
	],
	"scripts": {
		"prepack": "nuxt-module-build build",
		"prepack:stub": "nuxt-module-build build --stub"
	},
	"dependencies": {
		"@nuxt/kit": "3.13.2",
		"@nuxt/schema": "3.13.2",
		"@nuxt/ui": "next",
		"@nuxt/content": "^2.13.2",
		"@medplum/fhirtypes": "^3.2.17",
		"@nuxt/icon": "^1.5.6",
		"radix-vue": "^1.9.7",
		"fsh-sushi": "^3.11.1",
		"fhir-package-loader": "^1.0.0",
		"globby": "^14.0.2",
		"mdbox": "^0.1.0",
		"defu": "^6.1.4",
		"tailwindcss": "4.0.0-alpha.29",
		"tailwind-variants": "^0.2.1",
		"@nuxt/image": "^1.8.1",
		"@iconify/utils": "^2.1.33",
		"chokidar": "4.0.1",
		"fs-extra": "^11.2.0",
		"c12": "^2.0.1"
	},
	"devDependencies": {
		"typescript": "^5.6.2",
		"@nuxt/module-builder": "0.8.4",
		"@types/fs-extra": "11.0.4",
		"unbuild": "^2.0.0",
		"shiki": "^1.22.0"
	}
}